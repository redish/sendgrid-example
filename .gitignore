# Ignore bundler config.
/.bundle
vendor/bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*
!/log/.keep
/tmp
/public/uploads/*
!/public/uploads/.keep

# Not UP to Git, definition file system.
.env

# Ignore config files
config/database.yml
config/accounts.yml
config/deploy_user_config.yml

# capistrano
.capistrano/*

